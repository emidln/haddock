{

	"metadata": {
		"name": "HawkOwlAPIExample",
		"friendlyName": "HawkOwl's API Example",
		"versions": [1, 2]
	},

	"api": [
		{
			"name": "getWeather",
			"friendlyName": "Get Weather",
			"description": "Gets the weather.",
			"allowedMethods": ["GET"],
			"endpoint": "/weather",
			"processors": [
				{	
					"versions": [1],
					"optionalParams": ["countryCode"],
					"requiredParams": ["postcode", "unixTimestamp"]
				},
				{
					"versions": [2],
					"requiredParams": ["postcode", "countryCode", "unixTimestamp"],
					"returnParams": ["temperature", "windSpeed", "isRaining"]
				}
			]
		},
		{
			"name": "getMail",
			"friendlyName": "Get Mail",
			"description": "Gets your mail for you.",
			"allowedMethods": ["GET"],
			"endpoint": "/mail",
			"processors": [
				{
					"versions": [1, 2],
					"requiredParams": ["to"],
					"returnFormat": "list",
					"returnParams": ["to", "from", "subject", "content", "sentTimestamp"]
				}
			]
		}
	]

}
